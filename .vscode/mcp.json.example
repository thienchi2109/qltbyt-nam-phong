{
  "inputs": [
    {
      "type": "promptString",
      "id": "supabase-access-token",
      "description": "Supabase personal access token",
      "password": true
    }
  ],
  "servers": {
    "supabase-macos-linux": {
      "command": "bash",
      "args": [
        "-lc",
        "npx -y @supabase/mcp-server-supabase@latest --read-only --project-ref=cdthersvldpnlbvpufrr"
      ],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "${input:supabase-access-token}"
      }
    },
    "supabase-windows": {
      "command": "cmd",
      "args": [
        "/c",
        "npx -y @supabase/mcp-server-supabase@latest --read-only --project-ref=cdthersvldpnlbvpufrr"
      ],
      "env": {
        "SUPABASE_ACCESS_TOKEN": ""
      }
    },
    "supabase-write-mode (USE WITH CAUTION)": {
      "command": "bash",
      "args": [
        "-lc",
        "npx -y @supabase/mcp-server-supabase@latest --project-ref=cdthersvldpnlbvpufrr"
      ],
      "env": {
        "SUPABASE_ACCESS_TOKEN": "${input:supabase-access-token}"
      }
    }
  }
}